OSEK OSEK {

	OS ExampleOS {
		STATUS = CT_STATUS;
		PRETASKHOOK = FALSE;
		POSTTASKHOOK = FALSE;
		STARTUPHOOK = FALSE;
		ERRORHOOK = FALSE;
		SHUTDOWNHOOK = FALSE;
	};

	APPMODE std {};

  	TASK t1 {
		AUTOSTART = TRUE { 
			APPMODE = std; 
		};
		PRIORITY = 1;
		ACTIVATION = 1;
		SCHEDULE = NON;
	};

	COUNTER Counter1 {
		MAXALLOWEDVALUE = 25;
		TICKSPERBASE = 10;
		MINCYCLE = 2;
	};

	ALARM Alarm1 {
		COUNTER = Counter1;
		ACTION = ACTIVATETASK {
			TASK = t1;
		};
		AUTOSTART = FALSE;
	};
};
